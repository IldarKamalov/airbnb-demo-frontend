{
  "name": "airbnb-demo-frontend",
  "version": "0.1.0",
  "private": true,
  "now": {
    "name": "airbnb-demo-frontend",
    "alias": "ikamalov-airbnb-demo"
  },
  "dependencies": {
    "flexboxgrid2": "^7.0.0-alpha12",
    "google-map-react": "^0.27.0",
    "lodash": "^4.17.5",
    "normalize.css": "^7.0.0",
    "react": "^16.2.0",
    "react-dates": "^16.0.2",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "styled-components": "^2.4.0",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && rm ./build/static/js/*.js.map",
